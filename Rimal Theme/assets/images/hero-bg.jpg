<?xml version="1.0" encoding="UTF-8"?>
<svg width="1920" height="1080" viewBox="0 0 1920 1080" version="1.1" xmlns="http://www.w3.org/2000/svg">
    <defs>
        <!-- Desert Gradient -->
        <linearGradient id="desert-sky" x1="0%" y1="0%" x2="0%" y2="100%">
            <stop offset="0%" style="stop-color:#2c3e50"/>
            <stop offset="100%" style="stop-color:#3498db"/>
        </linearGradient>
        
        <!-- Sand Dunes Pattern -->
        <pattern id="dunes" x="0" y="0" width="400" height="200" patternUnits="userSpaceOnUse">
            <path d="M0,150 Q100,50 200,150 T400,150" fill="#d35400" opacity="0.8"/>
            <path d="M0,180 Q100,80 200,180 T400,180" fill="#e67e22" opacity="0.6"/>
        </pattern>
        
        <!-- Stars Pattern -->
        <pattern id="stars" x="0" y="0" width="100" height="100" patternUnits="userSpaceOnUse">
            <circle cx="10" cy="10" r="1" fill="#fff" opacity="0.5"/>
            <circle cx="50" cy="50" r="1" fill="#fff" opacity="0.7"/>
            <circle cx="90" cy="30" r="1" fill="#fff" opacity="0.6"/>
            <circle cx="30" cy="70" r="1" fill="#fff" opacity="0.4"/>
            <circle cx="70" cy="90" r="1" fill="#fff" opacity="0.5"/>
        </pattern>
    </defs>
    
    <!-- Sky Background -->
    <rect width="1920" height="1080" fill="url(#desert-sky)"/>
    
    <!-- Stars -->
    <rect width="1920" height="540" fill="url(#stars)"/>
    
    <!-- Desert Dunes -->
    <rect y="540" width="1920" height="540" fill="url(#dunes)"/>
    
    <!-- Moon -->
    <circle cx="1600" cy="200" r="80" fill="#fff" opacity="0.9"/>
    <circle cx="1580" cy="180" r="70" fill="#2c3e50" opacity="0.1"/>
    
    <!-- Warriors Silhouettes -->
    <g transform="translate(200, 600)" fill="#000" opacity="0.8">
        <!-- Multiple warrior silhouettes -->
        <path d="M0,0 l20,-40 l10,40 l15,-30 l5,30 h30 l5,-30 l15,30 l10,-40 l20,40 Z"/>
        <path d="M150,0 l20,-40 l10,40 l15,-30 l5,30 h30 l5,-30 l15,30 l10,-40 l20,40 Z"/>
        <path d="M300,0 l20,-40 l10,40 l15,-30 l5,30 h30 l5,-30 l15,30 l10,-40 l20,40 Z"/>
    </g>
</svg>