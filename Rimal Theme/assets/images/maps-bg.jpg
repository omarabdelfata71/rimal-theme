<?xml version="1.0" encoding="UTF-8"?>
<svg width="1920" height="1080" viewBox="0 0 1920 1080" version="1.1" xmlns="http://www.w3.org/2000/svg">
    <defs>
        <!-- Parchment Texture -->
        <pattern id="parchment" x="0" y="0" width="100" height="100" patternUnits="userSpaceOnUse">
            <rect width="100" height="100" fill="#d1a683"/>
            <circle cx="50" cy="50" r="40" fill="#c49c7c" opacity="0.5"/>
            <path d="M0,0 L100,100 M0,100 L100,0" stroke="#b38b6d" stroke-width="0.5" opacity="0.3"/>
        </pattern>
        
        <!-- Map Grid Pattern -->
        <pattern id="map-grid" x="0" y="0" width="50" height="50" patternUnits="userSpaceOnUse">
            <path d="M50,0 L0,0 0,50" stroke="#8b7355" stroke-width="0.5" fill="none" opacity="0.2"/>
        </pattern>
        
        <!-- Compass Rose -->
        <symbol id="compass-rose" viewBox="-100 -100 200 200">
            <g stroke="#6d5d4b" fill="none">
                <circle r="90"/>
                <circle r="70"/>
                <circle r="50"/>
                
                <!-- Cardinal Directions -->
                <path d="M0,-90 L0,90 M-90,0 L90,0" stroke-width="2"/>
                
                <!-- Intercardinal Directions -->
                <path d="M-63.6,-63.6 L63.6,63.6 M-63.6,63.6 L63.6,-63.6" stroke-width="1" opacity="0.7"/>
                
                <!-- Direction Labels -->
                <text x="0" y="-70" fill="#6d5d4b" text-anchor="middle" font-family="serif" font-size="20">N</text>
                <text x="70" y="0" fill="#6d5d4b" text-anchor="middle" font-family="serif" font-size="20">E</text>
                <text x="0" y="70" fill="#6d5d4b" text-anchor="middle" font-family="serif" font-size="20">S</text>
                <text x="-70" y="0" fill="#6d5d4b" text-anchor="middle" font-family="serif" font-size="20">W</text>
            </g>
        </symbol>
    </defs>
    
    <!-- Background -->
    <rect width="1920" height="1080" fill="url(#parchment)"/>
    
    <!-- Map Grid -->
    <rect width="1920" height="1080" fill="url(#map-grid)"/>
    
    <!-- Terrain Features -->
    <g opacity="0.3">
        <!-- Mountains -->
        <path d="M200,800 L400,400 600,800 M800,700 L1000,300 1200,700" stroke="#8b7355" stroke-width="3" fill="none"/>
        
        <!-- Rivers -->
        <path d="M300,200 Q600,400 400,600 T700,800" stroke="#6d5d4b" stroke-width="2" fill="none"/>
        
        <!-- Forests -->
        <g transform="translate(1400,500)">
            <circle cx="0" cy="0" r="100" stroke="#8b7355" stroke-width="1" fill="none"/>
            <circle cx="-50" cy="-50" r="80" stroke="#8b7355" stroke-width="1" fill="none"/>
            <circle cx="50" cy="50" r="80" stroke="#8b7355" stroke-width="1" fill="none"/>
        </g>
    </g>
    
    <!-- Compass Rose -->
    <use href="#compass-rose" x="1720" y="180" width="300" height="300" opacity="0.8"/>
    
    <!-- Map Border -->
    <rect x="40" y="40" width="1840" height="1000" stroke="#6d5d4b" stroke-width="4" fill="none"/>
    
    <!-- Decorative Corners -->
    <g fill="#6d5d4b" opacity="0.8">
        <path d="M20,20 L100,20 L100,40 L40,40 L40,100 L20,100 Z"/>
        <path d="M1820,20 L1900,20 L1900,100 L1880,100 L1880,40 L1820,40 Z"/>
        <path d="M20,980 L20,900 L40,900 L40,960 L100,960 L100,980 Z"/>
        <path d="M1820,960 L1880,960 L1880,900 L1900,900 L1900,980 L1820,980 Z"/>
    </g>
</svg>